{
  "name": "agent-qa-service",
  "version": "1.0.0",
  "description": "Agente aut√¥nomo de QA usando Gemini AI",
  "main": "dist/main.js",
  "scripts": {
    "start": "node dist/main.js",
    "start:bot": "ts-node src/slackBot.ts",
    "dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:db": "ts-node src/shared/database/test-connection.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "husky install",
    "migrate": "ts-node src/shared/database/migrate.ts",
    "simulate:agent": "ts-node src/testAgentFlow.ts"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.11.0",
    "@google/generative-ai": "^0.24.1",
    "@kubernetes/client-node": "^1.3.0",
    "@octokit/rest": "^22.0.0",
    "@slack/bolt": "^4.4.0",
    "@slack/types": "^2.14.0",
    "@slack/web-api": "^7.9.2",
    "@supabase/supabase-js": "^2.39.7",
    "@types/cors": "^2.8.19",
    "amqplib": "^0.10.3",
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "ioredis": "^5.3.2",
    "jira.js": "^5.2.1",
    "pg": "^8.11.3",
    "pgvector": "^0.2.0",
    "redis": "^5.5.6",
    "uuid": "^9.0.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/amqplib": "^0.10.7",
    "@types/express": "^5.0.3",
    "@types/google-cloud__storage": "^1.7.0",
    "@types/jest": "^29.5.10",
    "@types/node": "^22.15.30",
    "@types/pg": "^8.11.2",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^9.29.0",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1"
  }
